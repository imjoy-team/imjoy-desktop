# Name: ThunderSTORM

# Description:
#  ThunderSTORM: an ImageJ plugin for comprehensive PALM and STORM data analysis and super-resolution imaging

# Authors: Martin Ovesny (martin.ovesny at lf1.cuni.cz), Josef Borkovec (josef.borkovec at lf1.cuni.cz)
# Version: 1.3
# Date: 2013/3/4
# Requires: ImageJ 1.46r

Plugins>ThunderSTORM, "Camera setup", cz.cuni.lf1.lge.ThunderSTORM.CameraSetupPlugIn
Plugins>ThunderSTORM, "Run analysis", cz.cuni.lf1.lge.ThunderSTORM.AnalysisPlugIn("runAnalysis")
Plugins>ThunderSTORM, "Visualization", cz.cuni.lf1.lge.ThunderSTORM.RenderingPlugIn
Plugins>ThunderSTORM>3D calibration, "Cylindrical lens calibration", cz.cuni.lf1.lge.ThunderSTORM.CylindricalLensCalibrationPlugin
Plugins>ThunderSTORM>Performance testing, "Generator of simulated data", cz.cuni.lf1.lge.ThunderSTORM.DataGeneratorPlugIn
Plugins>ThunderSTORM>Performance testing, "Performance evaluation", cz.cuni.lf1.lge.ThunderSTORM.PerformanceEvaluationPlugIn
Plugins>ThunderSTORM>Import/Export, "Import results", cz.cuni.lf1.lge.ThunderSTORM.ImportExportPlugIn("import;results")
Plugins>ThunderSTORM>Import/Export, "Export results", cz.cuni.lf1.lge.ThunderSTORM.ImportExportPlugIn("export;results")
Plugins>ThunderSTORM>Import/Export, "Show results table", cz.cuni.lf1.lge.ThunderSTORM.results.TableHandlerPlugin
Plugins>ThunderSTORM>Import/Export, "-", -
Plugins>ThunderSTORM>Import/Export, "Import ground-truth", cz.cuni.lf1.lge.ThunderSTORM.ImportExportPlugIn("import;ground-truth")
Plugins>ThunderSTORM>Import/Export, "Export ground-truth", cz.cuni.lf1.lge.ThunderSTORM.ImportExportPlugIn("export;ground-truth")
Plugins>ThunderSTORM>Import/Export, "Show ground-truth table", cz.cuni.lf1.lge.ThunderSTORM.PerformanceEvaluationPlugIn("showGroundTruthTable")
Plugins>ThunderSTORM>Colocalization, "CBC", cz.cuni.lf1.lge.ThunderSTORM.CBCPlugIn
Plugins>ThunderSTORM, "-", -
Plugins>ThunderSTORM, "Check for updates...", cz.cuni.lf1.lge.ThunderSTORM.UpdaterPlugIn
Plugins>ThunderSTORM, "About ThunderSTORM...", cz.cuni.lf1.lge.ThunderSTORM.AboutPlugIn
